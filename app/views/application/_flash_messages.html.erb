<div class="row">
  <div class="col-md-offset-3 col-md-6">
    <% flash.each do |key, message| %>
      <div class="alert-dismissable <%= flash_class key %>" } >
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
        <p><%= message %></p>
      </div>
    <% end %>
  </div>
</div>
